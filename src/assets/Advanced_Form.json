[
    
    {
        "key":"Address",
        "type":"group",
       
        "options":{
            "children":[
                {
                "key":"name",
                "type":"input",
                "options":{
                    "label":"Full Name",
                    "placeholder":"Enter Your Name...",
                    "required":true,
                    "pattern":"^[A-Za-z]+\\s[A-Za-z]+$"
                    
                    
                }
            }, 
            {
                 "key":"Email",
                 "type":"input",
                 "options":{
                     "label":"Email",
                     "placeholder":"Enter Your Email...",
                     "required":true,
                     "pattern":"\\S+@\\S+\\.\\S+"
                  
                }
             }
            ]  
        }
    },
    {
        "key":"Phone",
        "type":"array",
        "options":{
            "children":[{
                "key":"phoneNumber",
                "type":"input",
                "options":{
                    "label":"Phone No.",
                    "placeholder":"Enter Your Phone No.(10 digit Only)...",
                   "required":true,
                   "pattern":"^(\\+91[\\-\\s]?)?[0]?(91)?[789]\\d{9}$"            
                }
             },
             {
                 "key":"Age",
                 "type":"input",
                 "options":{
                     "label":"My Age",
                     "placeholder":"Enter Your Age...",
                     "required":true,
                     "pattern":"^[1-9][0-9]?$|^100$"            
               }
            }
             
            ]
    }
 }
]